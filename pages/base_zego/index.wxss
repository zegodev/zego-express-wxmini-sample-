.containerBase {
  height: 63vh;
  padding: 10rpx;
}

live-pusher, live-player {
  width: 45vw;
  height: 45vw;
  margin: 5rpx;
}

.index-container {
  border-top: 1px slategrey solid
}
.input-container{
  text-align: center
}
